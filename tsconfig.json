{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "./app/",
		"paths": {
			// "@/*": [
			//   "./*",
			// ],
			"@components/*": [
				"./components/*"
			],
			"@hooks/*": [
				"./hooks/*"
			],
			"@constants/*": [
				"./constants"
			]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts"
	]
}